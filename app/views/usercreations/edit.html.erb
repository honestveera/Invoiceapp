<div class="jumbotron">
  <h3 class="text-info">Usercreation</h3>
<%= render 'form1' %>
<h3 class="text-success">UsercreationsDetails</h3>
<div class="display responsive no-wrap table-responsive">
<table class="table table-bordered table-hover sortable">
  <thead class="bg-success">
    <tr>
      <th class="text-info">Usertype</th>
      <th colspan="3"></th>
    </tr>
  </thead>
  <tbody>
    <% @usercreations.each do |usercreation| %>
      <tr>
        <td><%= usercreation.usertype %></td>
        <td><%= link_to 'SHOW', usercreation,:class=>"btn btn-primary btn-sm"%></td>
        <td><%= link_to 'EDIT', edit_usercreation_path(usercreation),:class=>"btn btn-info btn-sm"%></td>
        <td><%= link_to 'DESTROY', usercreation, method: :delete,:class=>"btn btn-danger btn-sm",data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
<br>
</div>
</div>
